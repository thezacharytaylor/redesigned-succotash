    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "axois": "0.0.1-security",
    "chalk": "5.2.0",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "daisyui": "2.46.0",
    "dayjs": "1.11.7",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fontfaceobserver": "2.3.0",
    "formik": "2.2.9",
    "husky": "8.0.2",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.1.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.1.0",
    "node-plop": "0.31.0",
    "plop": "3.1.1",
    "prettier": "2.8.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-is": "18.2.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.6.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "react-transition-group": "4.4.5",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.2.2",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "serve": "14.1.2",
    "shelljs": "0.8.5",
    "styled-components": "5.3.6",
    "stylelint": "14.16.1",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typescript": "4.9.4",
    "web-vitals": "3.1.0",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/eslint-plugin-tslint": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@typescript-eslint/typescript-estree": "5.47.1",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "cypress": "11.0.1",
    "json-server": "0.17.1",
    "json-server-auth": "2.1.0",
    "postcss": "8.4.20",
    "tailwindcss": "3.2.4"
  }
}
